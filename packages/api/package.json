{
    "name": "@t4/api",
    "version": "2.0.0",
    "main": "src/index.ts",
    "license": "MIT",
    "scripts": {
        "dev": "wrangler dev",
        "dev:remote": "wrangler dev --remote",
        "generate": "drizzle-kit generate:sqlite --schema=./src/db/schema.ts --out=./migrations",
        "migrate": "wrangler d1 migrations apply database",
        "migrate:local": "wrangler d1 migrations apply database --local",
        "seed": "wrangler d1 execute production --file=./seed/seed.sql",
        "seed:local": "wrangler d1 execute production --local --file=./seed/seed.sql",
        "studio": "drizzle-kit studio",
        "deploy": "wrangler deploy --minify src/worker.ts",
        "postinstall": "bun generate",
        "clean": "rm -rf .turbo node_modules"
    },
    "dependencies": {
        "@ai-sdk/openai": "^0.0.10",
        "@hono/trpc-server": "^0.1.0",
        "@langchain/community": "^0.0.54",
        "@langchain/openai": "^0.0.28",
        "@supabase/supabase-js": "^2.43.1",
        "@trpc/server": "^10.43.2",
        "ai": "^3.1.5",
        "cheerio": "^1.0.0-rc.12",
        "drizzle-orm": "^0.29.0",
        "drizzle-valibot": "beta",
        "groq-sdk": "^0.3.3",
        "hono": "^3.9.2",
        "html-to-text": "^9.0.5",
        "langchain": "^0.1.36",
        "miniflare": "3.20231025.1",
        "openai": "^4.44.0",
        "superjson": "1.13.3",
        "valibot": "^0.20.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20231025.0",
        "drizzle-kit": "^0.20.1",
        "typescript": "^5.2.2",
        "wrangler": "3.22.3"
    }
}
